{
    "compilerOptions": {
    "lib": ["WebWorker", "esnext"],
    "strict": true,
    "esModuleInterop": true,
    "types": ["./worker-configuration.d.ts"],
    "outDir": "dist",
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  },
    "include": ["src"]
}